package org.springextensions.actionscript.samples.cafetownsend.application {

	/**
	 * The names of the application events.
	 *
	 * @author Christophe Herreman
	 */
	public class ApplicationEvents {

		/** Dispatched when the user has successfully logged in. */
		public static const LOGGED_IN:String = "loggedIn";
		
		/** Dispatched when the user has successfully logged out. */
		public static const LOGGED_OUT:String = "loggedOut";
		
		/** Dispatched upon a request to log out. */
		public static const LOGOUT:String = "logout";

	}
}